RcBox<T> = {
    strong: i64,
    weak: i64,
    value: T,
}

Rc<T> = {
    inner: &RcBox<T>,
}

<T> Rc<T>: {
    new(value: T): Rc<T> {
        box: &RcBox<T> = alloc< RcBox<T> >(1)
        stack_box: RcBox<T> = 0 
        stack_box.strong = 1 
        stack_box.weak = 1
        stack_box.value = value
        box.write< RcBox<T> >(stack_box)
        
        rc: Rc<T> = 0
        rc.inner = box

        ; rc
    }
}

RcBox<T> = {
    strong: i64,
    weak: i64,
    value: T,
}

Rc<T> = {
    inner: &RcBox<T>,
}

<T> Rc<T>:: { 
    new(value: T): Rc<T> {
        box: &RcBox<T> = alloc< RcBox<T> >( 2 )

        box.write< RcBox<T> > (
            RcBox<T> { strong = 1, weak = 1, value = value }
        )
        
        rc: Rc<T> = Rc<T> { inner = box }

        ; rc
    }

    default(): Rc<T> {
        ; Rc<T>:new(T:default())
    }
}
